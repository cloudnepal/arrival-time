{
	"name": "arrival-time",
	"version": "1.0.3",
	"description": "Simple and powerful progress time estimation (ETA)",
	"keywords": [
		"backend",
		"frontend",
		"CLI",
		"web",
		"node",
		"eta",
		"estimate",
		"estimated",
		"time",
		"estimated time",
		"estimated-time",
		"progress",
		"progress eta",
		"progress-eta"
	],
	"license": "MIT",
	"author": "Robert Vitonsky <https://github.com/vitonsky>",
	"homepage": "https://github.com/vitonsky/arrival-time",
	"repository": {
		"type": "git",
		"url": "https://github.com/vitonsky/arrival-time"
	},
	"scripts": {
		"test": "jest",
		"test-fast": "TEST_FAST=true jest",
		"build": "rm -rf dist && rollup -c ./rollup.config.mjs",
		"release": "npm run build && cd dist && npm publish",
		"prettify": "prettier --write \"{*,**/*}.{ts,tsx,js,jsx}\" && eslint --fix \"{*,**/*}.{ts,tsx,js,jsx}\"",
		"packages:upgrade": "npm-check-updates -u",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.0.0",
		"@commitlint/config-conventional": "^17.0.0",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/jest": "^27.5.1",
		"@types/node": "^17.0.35",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
		"@typescript-eslint/parser": "^5.25.0",
		"eslint": "^8.40.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-simple-import-sort": "^10.0.0",
		"eslint-plugin-spellcheck": "^0.0.20",
		"eslint-plugin-unused-imports": "^2.0.0",
		"husky": "^8.0.1",
		"jest": "^28.1.0",
		"lint-staged": "^12.4.1",
		"npm-check-updates": "^16.10.9",
		"prettier": "^2.6.2",
		"rollup": "^4.20.0",
		"rollup-plugin-copy": "^3.5.0",
		"ts-jest": "^28.0.2",
		"typescript": "^4.6.4"
	}
}
